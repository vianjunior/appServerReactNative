{
  "expo": {
    "name": "appInicial",
    "slug": "appinicial",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled" : false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "devjuniorios.com.br",
      "buildNumber": "1",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package" : "devjunior.com.br",
      "icon": "./assets/icon.png",
      "versionCode": 2
    },
    "packagerOpts": {
      "nonPersistent": true
    }
  }
}